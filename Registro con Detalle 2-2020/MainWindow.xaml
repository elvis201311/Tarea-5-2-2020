<Window x:Class="Registro_con_Detalle_2_2020.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Registro_con_Detalle_2_2020"
        mc:Ignorable="d"
        Title="MainWindow" Height="300" Width="350">
    <Grid>

        <StackPanel Orientation="Vertical" HorizontalAlignment="Center" VerticalAlignment="Center">
            <Label Content="Registrar Articulo" FontWeight="Bold" Height="60" FontSize="36" HorizontalAlignment="Center" />
            <StackPanel Orientation="Horizontal" Margin="0,20,0,0"/>
            <Button Click="Button_Click">Registrar Articulos</Button>
            <StackPanel Orientation="Horizontal"/>
        </StackPanel>
    </Grid>
</Window>
